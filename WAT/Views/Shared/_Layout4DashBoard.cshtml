<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=10,IE=Edge" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Engineering Bussiness Support</title>

    <link href="@Url.Content("~/Content/bootstrap/bootstrap.min.css")" rel="stylesheet" type="text/css" />

    <link href="@Url.Content("~/Content/jquery-ui.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/bootstrapfileinput.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/jquery-autocomplete/jquery.auto-complete.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Layout.css")" rel="stylesheet" type="text/css" />

    @Scripts.Render("~/bundles/modernizr")
    <script src="~/Scripts/jquery-1.12.4.min.js" type="text/javascript"></script>
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/jquery-ui")

    <link type="text/css" rel="stylesheet" href="~/Content/jquery.datatables.min.css" />
    <script type="text/javascript" src="~/Scripts/jquery.datatables.min.js"></script>
    <link href="~/Scripts/datatables/buttons.dataTables.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/datatables/buttons.html5.min.js"></script>
    <script src="~/Scripts/datatables/dataTables.buttons.min.js"></script>
    <script src="~/Scripts/datatables/jszip.min.js"></script>

    <script src="~/Scripts/bootstrap/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Scripts/ajaxfileupload.js" type="text/javascript"></script>
    <script src="~/Scripts/moment.min.js"></script>

    <script type="text/javascript" src="~/Scripts/canvg.js"></script>
    <script type="text/javascript" src="~/Scripts/copy2clipboard.js"></script>

    <script src="~/Scripts/highcharts-modal.js" type="text/javascript"></script>
    <script src="~/Scripts/highcharts.js"></script>
    <script src="~/Scripts/heatmap.js"></script>
    <script src="~/Scripts/highcharts-more.js"></script>
    <script src="~/Scripts/offline-exporting.js"></script>
    <script src="~/Scripts/exporting.js"></script>
    <script src="~/Scripts/highcharts/boost.js"></script>

    <link type="text/css" rel="stylesheet" href="~/Scripts/bootstrap-tagsinput/bootstrap-tagsinput.css" />
    <script type="text/javascript" src="~/Scripts/bootstrap-tagsinput/bootstrap-tagsinput.js"></script>
    <script type="text/javascript" src="~/Scripts/bootstrap-tagsinput/bootstrap3-typeahead.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery-autocomplete/jquery.auto-complete.min.js"></script>
    <script src="~/Scripts/jquery-bootstrap-loading.js" type="text/javascript"></script>
    <link type="text/css" rel="stylesheet" href="~/Scripts/bootstrap-datepicker/bootstrap-datepicker.min.css" />
    <script type="text/javascript" src="~/Scripts/bootstrap-datepicker/bootstrap-datepicker.min.js"></script>
    <script type="text/javascript">
        $(function () {
            HighChartModal.init();
        });
    </script>

    <script src="../../ckeditor/ckeditor.js"></script>
    <script src="~/Scripts/jquery.base64.js"></script>
</head>

@{
    var isvideook = true;
    var ua = Request.UserAgent;
    if (ua.IndexOf("MSIE ") > 0)
    { isvideook = false; }
    if (ua.IndexOf("Edge/") > 0 || ua.IndexOf("Trident/") > 0)
    { isvideook = true; }
}

<body>
    <script type="text/javascript">
        $(function () {
            $('[data-toggle="popover"]').popover({ html: true, placement: 'bottom' });
        })
    </script>

    <div class="navbar navbar-default navbar-fixed-top div-nav">
        <nav class="navbar navbar-default navbar-fixed-top nav-content">
            <div class="container">
                <div class="row">
                    <div class="navbar-header logo">
                        <a class="navbar-brand" href="#">
                            <img alt="Brand" src="~/Content/images/FinisarLogo.jpg" height="54" width="250">
                        </a>
                    </div>
                    <div id="navbar" class="navbar-collapse collapse div-navlink">
                        <ul class="nav navbar-nav">
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button">Dash Board<span class="caret"></span></a>
                                <ul class="dropdown-menu">
                                    <li>
                                        @Html.ActionLink("WUXI WAT WIP", "WUXIWATWIPDS", "DashBoard")
                                    </li>
                                    <li>
                                        @Html.ActionLink("WAT YIELD 4 PLANNING", "WeeklyYield4Planning", "DashBoard")
                                    </li>
                                    <li>
                                        @Html.ActionLink("WUXI WAT CAPACITY", "WeeklyCapacityDS", "DashBoard")
                                    </li>
                                    <li>
                                        @Html.ActionLink("OVEN RESOURCE PREDICT", "WATCapPredict", "DashBoard")
                                    </li>
                                </ul>
                            </li>
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button">Manage Tool<span class="caret"></span></a>
                                <ul class="dropdown-menu">
                                    <li>
                                        @Html.ActionLink("WUXI WAT STATUS", "WUXIWATStatus", "MGTL")
                                    </li>
                                    <li>
                                        @Html.ActionLink("WAT DAILY CHECK", "WATJOBCheck", "DashBoard")
                                    </li>
                                    <li>
                                        @Html.ActionLink("WAT CHECK HISTORY", "WATCheckHistory", "DashBoard")
                                    </li>
                                    <li>
                                        @Html.ActionLink("WAT RT YIELD", "WATRTYield", "DashBoard")
                                    </li>
                                    <li>
                                        @Html.ActionLink("WUXI WAT YIELD", "WeeklyYieldDS", "DashBoard")
                                    </li>
                                </ul>
                            </li>
                        </ul>


                        @*<ul class="nav navbar-nav">
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                                    <span class="glyphicon glyphicon-link" title="Friendly Links" data-toggle="tooltip"></span>
                                </a>
                                <ul class="dropdown-menu">
                                    <li>
                                        <a href="http://cn-traceview/" target="_blank">Trace View</a>
                                    </li>
                                </ul>
                            </li>
                        </ul>*@
                    </div>
                </div>
            </div>
        </nav>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - VCSEL WAT Support</p>
        </footer>
        <script type="text/javascript">
            function SetReplyPeople(reply) {
                CKEDITOR.instances.editor1.setData(reply + '&nbsp;');
                $('#commentinput').collapse('show');
                var scrollingElement = (document.scrollingElement || document.body)
                scrollingElement.scrollTop = scrollingElement.scrollHeight;
            }

        </script>
    </div>

</body>
</html>

