
@{
    ViewBag.Title = "WATCheckHistory";
    Layout = "~/Views/Shared/_Layout4Product.cshtml";
}


<script type="text/javascript" src="~/Scripts/JobCheckList.js"></script>
<script type="text/javascript">
        $(function(){
            JOBCHECK.WATCHECKHISTORY();
        })
</script>

<style type="text/css">
    th.dt-center, td.dt-center {
        text-align: center;
        font-size: 12px !important;
    }

    tr.DONE {
        background-color: limegreen !important;
    }

    tr.PENDING {
        background-color: coral !important;
    }
</style>

<div class="row" style="margin-top:10px;">
    <div class="col-xs-12">
        <div class="v-container">
            <div class="v-filter">
                <div class="row row-margin">
                    <label class="col-xs-2">Start Date</label>
                    <div class="col-xs-3">
                        <div class="input-group date" data-date=""
                             data-date-format="yyyy-mm-dd">
                            <input type="text" class="form-control"
                                   id="sdate" name="sdate" value="" readonly>
                            <span class="input-group-addon">
                                <span class="glyphicon glyphicon-th"></span>
                            </span>
                        </div>
                    </div>
                    <div class="col-xs-1 box-search">
                        <input type="button" class="btn btn-success" name="btn-search" id="btn-search" value="Search">
                    </div>
                </div>
            </div>

            <div class="v-content">
                <div style="margin-left:-60px;margin-top:25px">
                    <table class="table table-hover  table-condensed table-striped" id="wafertable" style="margin-top:2%">
                        <caption class="tb-caption">WUXI WAT JOB CHECK HISTORY</caption>
                        <thead id="waferhead"></thead>
                        <tbody id="wafercontent"></tbody>
                    </table>
                </div>
            </div>

        </div>
    </div>
</div>

